{
  "name": "test-ecosystem-ci",
  "version": "0.0.1",
  "packageManager": "pnpm@10.21.0",
  "description": "Run CI across the ecosystem",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/gugustinette/ecosystem-ci#readme",
  "bugs": {
    "url": "https://github.com/gugustinette/ecosystem-ci/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugustinette/ecosystem-ci.git"
  },
  "author": "Augustin Mercier <gugustinette@proton.me>",
  "funding": "https://github.com/sponsors/gugustinette",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "bin": {
    "ecosystem-ci": "./dist/cli.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown && clear && node ./dist/cli.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "giget": "^2.0.0",
    "unconfig-core": "^7.4.1",
    "unrun": "^0.2.8"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.2.10",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "obug": "^0.1.3",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
